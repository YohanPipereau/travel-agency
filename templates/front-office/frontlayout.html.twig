{# templates/front-office/frontlayout.html.twig #}

{% extends "baselayout.html.twig" %}

{% block stylesheet %}
    <link rel="stylesheet"  href="{{ asset('css/animations.min.css') }}">
    <link rel="stylesheet"  href="{{ asset('css/font-awesome.css') }}">
    <link rel="stylesheet"  href="{{ asset('css/ionicons.css') }}">
    <link rel="stylesheet"  href="{{ asset('css/jquery.fancybox.css') }}">
    <link rel="stylesheet"  href="{{ asset('js/vegas/jquery.vegas.min.css') }}">
    <link rel="stylesheet"  href="{{ asset('style.css') }}">
{% endblock %}

{% block menu %}
    <li class="active"><a href="/">HOME</a></li>
    <li><a href="/circuit">All Tours</a></li>
    <li><a href="/programmation">Programmed Tours</a></li>
    <li><a href="/admin"> Administration </a></li>
    <li><a href="/sign-in">Sign In</a></li>
    <li><a href="/sign-up">Sign Up</a></li>
{% endblock %}

{% block body %}
<!-- start upcoming tours section -->
<section id="how-to-use">
     <div class="container">
          <div class="row text-center header">
               <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                   <h3> {% block sectiontitle %} {% endblock %} </h3>
                    <hr>
               </div>
          </div>

<div class="container">
<div class="row text-center" data-anim-type="fade-in-up">
<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 col-md-offset-4 text-left">
    {% block sectioncontent %}
    {% endblock %}
</div>
</div>
</div>
    </div>
</section>
<!--end upcoming tours section -->


{% endblock %}

{% block footpage %}
<!-- start of contact section-->
<section id="contact">
     <div class="container">
       <div class="row text-center header">
         <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
           <h3>CONTACT DETAILS</h3>
           <hr>
         </div>

        <div class="col-xs-12 col-sm-4 col-md-6 col-lg-6">
         <div class="contact-wrapper">
          <h3>Quick Contact</h3>
              <h4><strong>Email : </strong> yohan.pipereau@example.com </h4>
               <h4><strong>Call : </strong> +33600000000 </h4>
              <h4><strong>Skype : </strong> ypipereau </h4>
          </div>
        </div>

        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
            <div class="contact-wrapper">
                <h3>Address : </h3>
                <h4>9, rue Charles Fourier </h4>
                <h4>Evry, 91000</h4>
                <h4>France</h4>
            </div>
        </div>

       </div>
     </div>
</section>
<!-- end of contact section-->
{% endblock %}


{% block custompage_script %}

<script src="{{ asset('vendor/components/jquery/jquery.js') }}"></script>
<script src="{{ asset('vendor/twbs/bootstrap/dist/js/bootstrap.js') }}"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
<script  src="{{ asset('js/custom.js')}}"></script>
<script  src="{{ asset('js/jquery.isotope.js') }}"></script>
<script  src="{{ asset('js/jquery.easing.min.js') }}" ></script>
<script  src="{{ asset('js/animations.min.js') }}"> </script>
<script  src="{{ asset('js/vegas/jquery.vegas.min.js') }}"></script>
<script  src="{{ asset('js/source/jquery.fancybox.js') }}"></script>
<script>
$(document).ready(function(){
    $('[data-toggle="tooltip"]').tooltip();
});
</script>
{% endblock %}
